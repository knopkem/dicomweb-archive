{
  "name": "dicomweb-archive",
  "version": "1.2.0",
  "description": "Lightweight dicomweb server with sql db backend support",
  "author": "Michael Knopke",
  "private": false,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:swc": "nest start -b swc --type-check",
    "start:fast": "nest start -b swc",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knopkem/dicomweb-archive.git"
  },
  "bugs": {
    "url": "https://github.com/knopkem/dicomweb-archive/issues"
  },
  "homepage": "https://github.com/knopkem/dicomweb-archive#readme",
  "keywords": [
    "DICOMWEB",
    "SERVER",
    "PACS",
    "ARCHIVE",
    "DIMSE",
    "DICOM",
    "STORE",
    "MOVE",
    "GET",
    "FIND",
    "SCP",
    "SCU",
    "OHIF",
    "VIEWER",
    "ORTHANC",
    "DCMTK",
    "DCM4CHEE",
    "PACSONE",
    "CONQUEST"
  ],
  "dependencies": {
    "@knopkem/dicom-parser": "^1.8.15",
    "@nestjs/common": "10.4.9",
    "@nestjs/core": "10.4.9",
    "@nestjs/platform-express": "10.4.14",
    "@nestjs/serve-static": "4.0.2",
    "@nestjs/typeorm": "10.0.2",
    "@types/express-serve-static-core": "5.0.2",
    "better-sqlite3": "9.6.0",
    "class-transformer": "^0.5.1",
    "class-validator": "0.14.1",
    "dcmjs-dimse": "0.1.30",
    "dicom-data-dictionary": "^0.3.1",
    "helmet": "8.0.0",
    "mariadb": "3.4.0",
    "mysql2": "3.11.4",
    "pg": "8.13.1",
    "recursive-readdir-async": "^1.3.0",
    "reflect-metadata": "0.2.2",
    "rimraf": "6.0.1",
    "rxjs": "^7.8.1",
    "typeorm": "0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "10.4.8",
    "@nestjs/schematics": "10.2.3",
    "@nestjs/testing": "10.4.9",
    "@swc/cli": "0.5.1",
    "@swc/core": "1.9.3",
    "@types/async-eventemitter": "0.2.4",
    "@types/express": "5.0.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.10.0",
    "@types/supertest": "6.0.2",
    "@types/validator": "13.12.2",
    "@typescript-eslint/eslint-plugin": "8.16.0",
    "@typescript-eslint/parser": "8.16.0",
    "eslint": "9.15.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "jest": "29.7.0",
    "prettier": "3.4.0",
    "supertest": "7.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
