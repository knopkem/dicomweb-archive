{
  "name": "dicomweb-archive",
  "version": "1.1.0",
  "description": "Lightweight dicomweb server with sql db backend support",
  "author": "Michael Knopke",
  "private": false,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knopkem/dicomweb-archive.git"
  },
  "bugs": {
    "url": "https://github.com/knopkem/dicomweb-archive/issues"
  },
  "homepage": "https://github.com/knopkem/dicomweb-archive#readme",
  "keywords": [
    "DICOMWEB",
    "SERVER",
    "PACS",
    "ARCHIVE",
    "DIMSE",
    "DICOM",
    "STORE",
    "MOVE",
    "GET",
    "FIND",
    "SCP",
    "SCU",
    "OHIF",
    "VIEWER",
    "ORTHANC",
    "DCMTK",
    "DCM4CHEE",
    "PACSONE",
    "CONQUEST"
  ],
  "dependencies": {
    "@knopkem/dicom-parser": "^1.8.15",
    "@nestjs/common": "^9.3.2",
    "@nestjs/core": "^9.3.2",
    "@nestjs/platform-express": "^9.3.2",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "@types/express-serve-static-core": "^4.17.33",
    "better-sqlite3": "^8.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dicom-data-dictionary": "^0.3.1",
    "mariadb": "^3.0.2",
    "mysql2": "^2.2.5",
    "pg": "^8.9.0",
    "recursive-readdir-async": "^1.3.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.2",
    "rxjs": "^7.8.0",
    "typeorm": "^0.3.11"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.12",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.1",
    "prettier": "^2.8.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
